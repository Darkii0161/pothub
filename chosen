if gameName == "Deepwoken" then
	game.Workspace.Live.ChildAdded:Connect(function(player)
		if player == player.Name then
			pcall(function()
				loadstring(game:HttpGet(scr1))()
			end)
		end
	end)
end

local on = false
UserInputService.InputBegan:connect(function(player, p9)
	if p9 then
		return
	end
	if player.KeyCode == Enum.KeyCode.RightAlt then
		if hashiden == false then
			hashiden = true
		end
		HubGui.Enabled = not HubGui.Enabled
		on = HubGui.Enabled
	end
end)

spawn(function()
	task.wait(1)
	while true do
		task.wait(1.75)
		if not game.Players.LocalPlayer.PlayerGui:FindFirstChild(HubGui.Name) then
			game.Players.LocalPlayer:Kick("gui was deleted??")
		end
	end
end)
